{
  "name": "corbosman/laravel-passport-claims",
  "description": "Add claims to Laravel Passport JWT Tokens",
  "license": "MIT",
  "authors": [
    {
      "name": "Cor Bosman",
      "email": "cor@in.ter.net"
    }
  ],
  "homepage": "https://github.com/corbosman/laravel-passport-claims",
  "keywords": [
    "Laravel",
    "passport",
    "jwt",
    "claim",
    "laravel-passport-claims"
  ],
  "require": {
    "illuminate/support": "^11.0|^12.0",
    "laravel/passport": "^11.2|^12.0|^13.0",
    "nesbot/carbon": "^3"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9.0|^10.0",
    "phpseclib/phpseclib": "^3.0",
    "phpseclib/phpseclib2_compat": "^1.0",
    "pestphp/pest": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "CorBosman\\Passport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CorBosman\\Passport\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CorBosman\\Passport\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true
    }
  }
}
