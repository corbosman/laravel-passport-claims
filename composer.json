{
    "name": "corbosman/laravel-passport-claims",
    "description": "Add claims to Laravel Passport JWT Tokens",
    "license": "MIT",
    "authors": [
        {
            "name": "Cor Bosman",
            "email": "cor@in.ter.net"
        }
    ],
    "homepage": "https://github.com/corbosman/laravel-passport-claims",
    "keywords": [
        "Laravel",
        "passport",
        "jwt",
        "claim",
        "laravel-passport-claims"
    ],
    "require": {
        "illuminate/support": "^9.0|^10.0|^11.0",
        "laravel/passport": "^10.3|^11.2|dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10|^10.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpseclib/phpseclib": "^3.0",
        "phpseclib/phpseclib2_compat": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CorBosman\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CorBosman\\Passport\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CorBosman\\Passport\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
