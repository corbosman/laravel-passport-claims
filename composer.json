{
    "name": "corbosman/laravel-passport-claims",
    "description": "Add claims to Laravel Passport JWT Tokens",
    "license": "MIT",
    "authors": [
        {
            "name": "Cor Bosman",
            "email": "cor@in.ter.net"
        }
    ],
    "homepage": "https://github.com/corbosman/laravel-passport-claims",
    "keywords": ["Laravel", "passport" ,"jwt" , "claim", "laravel-passport-claims"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8",
        "laravel/passport": "^9.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "CorBosman\\Passport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CorBosman\\Passport\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CorBosman\\Passport\\ServiceProvider"
            ]
        }
    }
}
